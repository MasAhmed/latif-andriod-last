<resources xmlns:tools="http://schemas.android.com/tools">    <!-- Base application theme. -->    <style name="Theme.Latif" parent="Theme.MaterialComponents.DayNight.NoActionBar">        <!-- Primary brand color. -->        <item name="colorPrimary">@color/sunset_orange</item>        <item name="colorPrimaryVariant">@color/sunset_orange</item>        <item name="colorOnPrimary">@color/white</item>        <!-- Secondary brand color. -->        <item name="colorSecondary">@color/sunset_orange</item>        <item name="colorSecondaryVariant">@color/sunset_orange</item>        <item name="colorOnSecondary">@color/black</item>        <item name="android:windowAnimationStyle">@style/CustomActivityAnimation</item>        <!-- Status bar color. -->        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>        <!-- Customize your theme here. -->    </style>    <style name="NavigationTheme">        <item name="android:layout_marginBottom">70dp</item>        <item name="android:layout_marginTop">70dp</item>    </style>    <style name="Theme.Latif.NoActionBar">        <item name="windowActionBar">false</item>        <item name="windowNoTitle">true</item>    </style>    <style name="Theme.Latif.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />    <style name="Theme.Latif.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" /></resources>